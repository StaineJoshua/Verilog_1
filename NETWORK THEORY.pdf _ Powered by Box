    ); --button-background-color-active: color-mix(
      in srgb,
      currentColor 21%,
      transparent
    ); --color-accent-primary: #0df; --color-accent-primary-hover: #80ebff; --color-accent-primary-active: #aaf2ff; --border-interactive-color: #bfbfc9; --color-canvas: #1c1b22; }
}

